pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                script {
                  def props = readFile(file: "books.xml")
                  echo "$props"
                  def xml = new XmlSlurper().parseText(props)
                  def env = xml.catalog.title[1].title[0]

                  echo "${env}"
                }
            }
        }
    }
}
