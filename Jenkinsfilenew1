node {

 

    
    properties([
      parameters([
        string(name: 'serviceName', defaultValue: '', description: 'Please enter the serverless repository name'),
        
      ])
    ])
    def serviceRepoUrl = "https://github.wm.com/esb/${params.serviceName}.git"
    stage('Results') {
      echo "$serviceRepoUrl"
    }
}
