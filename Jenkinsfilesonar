pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                script {
                  echo "Hello"
                  def props = readFile(file: "resources/sonar-proj.properties")
                  echo "$props"
                  
                  if (fileExists('resources/sonar-proj.propert')) {
                    echo 'Yes'
                  } else {
                    echo 'No'
                  }
                }
            }
        }
    }
}
